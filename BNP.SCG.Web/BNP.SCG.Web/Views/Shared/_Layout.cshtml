@using System.Security.Claims
@{
    var _userFName = ((ClaimsIdentity)User.Identity).Claims.Where(c => c.Type == "UserFirstName").Select(c =>
            c.Value).FirstOrDefault();
    var _userLName = ((ClaimsIdentity)User.Identity).Claims.Where(c => c.Type == "UserLastName").Select(c =>
    c.Value).FirstOrDefault();
    var _userId = ((ClaimsIdentity)User.Identity).Claims.Where(c => c.Type == "UserId").Select(c =>
    c.Value).FirstOrDefault();
    var _userRole = ((ClaimsIdentity)User.Identity).Claims.Where(c => c.Type == ClaimTypes.Role).Select(c =>
    c.Value).FirstOrDefault();

    var successMessages = TempData["SuccessMessages"] as String;
    var errMessages = TempData["ErrorMessages"] as String;
}

