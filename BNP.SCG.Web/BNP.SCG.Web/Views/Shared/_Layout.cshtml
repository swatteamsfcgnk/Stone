            }

            var _notimessage_updateduser = "@TempData["NotiMessageUpdatedUser"]";
            if (_notimessage_updateduser.length > 0 && _notimessage_updateduser != "" && _notimessage_updateduser != undefined) {

                Swal.fire({
                    type: 'info',
                    title: `ข้อมูลแนะนำ`,
                    html: _notimessage_updateduser,
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'ยืนยัน!',
                    confirmButtonClass: 'btn btn-primary',
                    cancelButtonClass: 'btn btn-danger ml-1',
                    cancelButtonText: 'ยกเลิก',
                    buttonsStyling: false,
                }).then((result) => {
                    if (result.value) {
                        "@TempData.Remove("NotiMessageUpdatedUser")";
                        $.get('@Url.Action("Logout", "Account")', function () {
                            location.reload();
                        });
                    }
                })
                "@TempData.Remove("NotiMessageUpdatedUser")";
            }

        });
    </script>

    @RenderSection("Scripts", required: false)


</body>
